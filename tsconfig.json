{
  "compilerOptions": {
    "target": "es2016",
    "module": "esnext",
    "strict": true,
    "noImplicitAny": true,
    "moduleResolution": "node",
    "skipLibCheck": true,
    "declaration": true,
    "allowSyntheticDefaultImports": true,
    "forceConsistentCasingInFileNames": true,
    "outDir": "dist",
    "types": ["@cloudflare/workers-types", "jest"]
  },
  "include": ["src"],
  "exclude": ["node_modules"]
}
