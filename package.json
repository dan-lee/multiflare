{
  "version": "0.2.1",
  "engines": {
    "node": ">=16"
  },
  "type": "module",
  "author": "dan-lee",
  "license": "MIT",
  "repository": "https://github.com/dan-lee/multiflare",
  "name": "multiflare",
  "bin": "dist/cli.js",
  "main": "dist/multiflare.js",
  "types": "dist/multiflare.d.ts",
  "module": "dist/multiflare.js",
  "files": [
    "dist",
    "patches"
  ],
  "exports": {
    ".": {
      "import": "./dist/multiflare.js"
    },
    "./utils/kv": {
      "import": "./dist/utils/kv.js"
    },
    "./*": {
      "import": "./dist/*.js"
    }
  },
  "dependencies": {
    "@iarna/toml": "3.0.0",
    "commander": "8.3.0",
    "esbuild": "0.13.14",
    "match-url-wildcard": "0.0.4",
    "miniflare": "2.0.0-rc.1",
    "tiny-glob": "0.2.9"
  },
  "scripts": {
    "build": "rm -rf dist && tsc",
    "multiflare": "node --experimental-vm-modules --loader @erwink/esb src/cli.ts",
    "example": "yarn multiflare example/multiflare/workers",
    "test": "node --experimental-vm-modules node_modules/.bin/jest",
    "postinstall": "patch-package",
    "prepublishOnly": "yarn test && yarn build"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "3.2.0",
    "@erwink/esb": "1.0.0-next.12",
    "@types/jest": "27.0.3",
    "@types/node": "16.11.9",
    "esbuild-jest": "0.5.0",
    "jest": "27.3.1",
    "jest-environment-miniflare": "2.0.0-rc.1",
    "patch-package": "6.4.7",
    "prettier": "2.4.1",
    "typescript": "4.5.2",
    "undici": "4.10.2"
  }
}
